{
  "name": "@wovosoft/wovoui",
  "version": "0.4.42",
  "type": "module",
  "scripts": {
    "build": "vite build && npm run generate:types",
    "build:only": "vite build",
    "dev": "vite --config demo-vite.config.ts",
    "build:demo": "vite build --config demo-vite.config.ts",
    "serve": "vite preview",
    "generate:types": "vue-tsc --declaration --emitDeclarationOnly --outdir ./dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.7",
    "@vueuse/components": "^10.1.2",
    "@vueuse/core": "^10.1.2",
    "@wovosoft/wovoui-icons": "^1.1.9",
    "bootstrap": "^5.3.0-alpha3",
    "dayjs": "^1.11.7",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/types": "^7.21.5",
    "@types/node": "^20.1.1",
    "@vitejs/plugin-vue": "^4.2.1",
    "lodash": "^4.17.21",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vitepress": "^1.0.0-alpha.75",
    "vue-tsc": "^1.6.4"
  },
  "description": "Bootstrap 5 and Vue 3 Utility Components",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.umd.mjs",
  "module": "./dist/index.es.mjs",
  "exports": {
    "./src/index": "./src/index.mjs",
    "./dist/style.css": "./dist/style.css",
    "./types": "./dist/types/index.d.ts",
    ".": {
      "import": "./dist/index.es.mjs",
      "require": "./dist/index.umd.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wovosoft/wovoui.git"
  },
  "keywords": [
    "Bootstrap 5",
    "Vue 3",
    "Design",
    "Libray"
  ],
  "author": "Narayan Adhikary",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wovosoft/wovoui/issues"
  },
  "homepage": "https://github.com/wovosoft/wovoui#readme",
  "peerDependencies": {
    "axios": "*",
    "chart.js": "^3.9.1"
  }
}
