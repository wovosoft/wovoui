{
  "name": "@wovosoft/wovoui",
  "version": "0.2.29",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with_tags": "vite build && node vetur-tags-generate.js",
    "build:tags": "node vetur-tags-generate.js",
    "serve": "vite preview",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --declarationDir dist/types"
  },
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "@wovosoft/wovoui-icons": "^0.0.3",
    "axios": "^0.26.1",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.2",
    "chart.js": "^3.7.1",
    "dayjs": "^1.10.7",
    "highlight.js": "^11.4.0",
    "lodash": "^4.17.21",
    "vue": "^3.2.16",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "babel-loader": "^8.2.3",
    "glob": "^7.2.0",
    "sass": "^1.49.9",
    "vite": "^2.6.4",
    "vite-plugin-dts": "^0.9.9",
    "vue-docgen-api": "^4.44.17",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.32.1"
  },
  "description": "Bootstrap 5 and Vue 3 Utility Components",
  "files": [
    "dist",
    "src"
  ],
  "typings": "./dist/src/index.d.ts",
  "style": "./dist/style.css",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    "./src/index": "./src/index.js",
    "./dist/style": "./dist/style.css",
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "vetur": {
    "tags": "dist/vetur-tags.json",
    "attributes": "dist/vetur-attributes.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wovosoft/wovoui.git"
  },
  "keywords": [
    "Bootstrap",
    "5",
    "Vue",
    "3",
    "Design",
    "Libray"
  ],
  "author": "Narayan Adhikary",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wovosoft/wovoui/issues"
  },
  "homepage": "https://github.com/wovosoft/wovoui#readme"
}
