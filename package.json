{
  "name": "@wovosoft/wovoui",
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run generate:types",
    "generate:types": "vue-tsc --declaration --emitDeclarationOnly --outdir ./dist",
    "preview": "vite preview"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./src/index": "./src/index.js",
    "./dist/style.css": "./dist/style.css",
    "./types": "./dist/types/index.d.ts",
    "./composables": "./dist/composables/index.js",
    "./directives": "./dist/directives/index.js",
    "./shared": "./dist/shared/index.js"
  },
  "peerDependencies": {
    "axios": "*",
    "chart.js": "^4.3.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@vueuse/components": "^10.1.2",
    "@vueuse/core": "^10.1.2",
    "@wovosoft/wovoui-icons": "^1.1.9",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "dayjs": "^1.11.8",
    "lodash": "^4.17.21",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@vitejs/plugin-vue": "^4.1.0",
    "sass": "^1.62.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vue-router": "^4.2.2",
    "vue-tsc": "^1.4.2"
  }
}
