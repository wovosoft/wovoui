  "vetur": {
    "tags": "./vetur-tags.json",
    "attributes": "./vetur-attributes.json"
  },
  "peerDependencies": {
    "axios": "*",
    "chart.js": "^4.4.7",
    "vue": "^3.5.x",
    "prismjs": "^1.29.x",
    "dayjs": "^1.11.x",
    "bootstrap": "^5.3.x",
    "bootstrap-icons": "^1.11.x"
  },
