{
  "name": "@wovosoft/wovoui",
  "version": "0.4.26",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite --config demo-vite.config.ts",
    "build:demo": "vite build --config demo-vite.config.ts",
    "serve": "vite preview",
    "generate:types": "vue-tsc --declaration --emitDeclarationOnly --outdir ./dist"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "@vueuse/components": "^9.0.2",
    "@vueuse/core": "^9.0.2",
    "@wovosoft/wovoui-icons": "^1.1.4",
    "bootstrap": "^5.2.0",
    "dayjs": "^1.11.4",
    "vue": "^3.2.16",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@babel/types": "^7.20.2",
    "@types/node": "^18.6.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "lodash": "^4.17.21",
    "typescript": "^4.7.4",
    "vite": "^2.7.2",
    "vue-tsc": "^1.0.9"
  },
  "description": "Bootstrap 5 and Vue 3 Utility Components",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.umd.mjs",
  "module": "./dist/index.es.mjs",
  "exports": {
    "./src/index": "./src/index.mjs",
    "./dist/style.css": "./dist/style.css",
    "./types": "./dist/types/index.d.ts",
    ".": {
      "import": "./dist/index.es.mjs",
      "require": "./dist/index.umd.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wovosoft/wovoui.git"
  },
  "keywords": [
    "Bootstrap",
    "5",
    "Vue",
    "3",
    "Design",
    "Libray"
  ],
  "author": "Narayan Adhikary",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wovosoft/wovoui/issues"
  },
  "homepage": "https://github.com/wovosoft/wovoui#readme",
  "peerDependencies": {
    "axios": "*",
    "chart.js": "^3.7.1",
    "highlight.js": "^11.5.0"
  }
}
